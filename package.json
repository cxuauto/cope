{
  "name": "cope",
  "version": "1.0.0",
  "description": "test",
  "author": "lingdong <xxx@xxx.xxx>",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "npm install && electron-builder",
    "release": "npm install && electron-builder"
  },
  "extends": null,
  "build": {
    "appId": "cope",
    "files": [
      "dist/**/*",
      "package.json",
      "main.js"
    ],
    "mac": {
      "icon": "icon.icns",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "favicon-256.ico",
      "target": [
      {
        "target": "nsis",
        "arch": [
          "x64",
          "ia32"
        ]
      }, 
      {
        "target": "zip",
        "arch": [
          "x64",
          "ia32"
      ],
      {
        "target": "7z",
        "arch": [
          "x64",
          "ia32"
      ]
      }
      }
    ]
    },
    "linux": {
      "icon": "favicon-256.ico",
      "category": "Utility",
      "target": [
        "deb"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "umap-js": "^1.3.1"
  },
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": ">=21.0.14"
   }
}
